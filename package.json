    "koa-bodyparser": "^4.3.0",
    "koa-compose": "^4.1.0",
    "koa-router": "^10.1.1",
    "log4js": "^6.3.0",
    "node-fetch": "^3.0.0",
    "sequelize": "^6.8.0",
    "socket.io": "^4.3.1",
    "tedious": "^14.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/koa-bodyparser": "^4.3.3",
